<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">

    <servlet>
        <servlet-name>RequestHandler</servlet-name>
        <servlet-class>com.appnexus.integration.servlet.RequestHandlerServlet</servlet-class>
        <load-on-startup />
    </servlet>
    <servlet>
        <servlet-name>ReadyHandler</servlet-name>
        <servlet-class>com.appnexus.integration.servlet.ReadyHandlerServlet</servlet-class>
        <load-on-startup />
    </servlet>
    <servlet-mapping>
        <servlet-name>TractorHandler</servlet-name>
        <url-pattern>/tractorHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestHandler</servlet-name>
        <url-pattern>/BidHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestHandler</servlet-name>
        <url-pattern>/NotifyHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestHandler</servlet-name>
        <url-pattern>/PixelHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestHandler</servlet-name>
        <url-pattern>/ClickHandler</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>IsoBusDevice</servlet-name>
        <url-pattern>/isoBusHandler</url-pattern>
    </servlet-mapping>
       <servlet-mapping>
        <servlet-name>IsoAgroDevice</servlet-name>
        <url-pattern>/isoAgroDevice</url-pattern>
    </servlet-mapping>
</web-app>
